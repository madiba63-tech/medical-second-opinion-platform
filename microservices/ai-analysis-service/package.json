{
  "name": "@second-opinion/ai-analysis-service",
  "version": "1.0.0",
  "description": "AI Analysis Service - Medical document analysis, diagnosis assistance, and ML-powered insights",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t ai-analysis-service .",
    "docker:run": "docker run -p 3003:3003 ai-analysis-service"
  },
  "keywords": [
    "microservice",
    "ai-analysis",
    "medical",
    "machine-learning",
    "healthcare",
    "diagnosis"

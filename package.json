{
  "name": "second-opinion",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:check": "next lint --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "quality:check": "npm run typecheck && npm run lint:check && npm run format:check",
    "quality:fix": "npm run format && npm run lint:fix",
    "test": "jest",
    "test:watch": "node agents/test-manager.js watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:dev": "node agents/test-manager.js full",
    "test:health": "node agents/test-manager.js health",
    "standards:check": "npm run quality:check",
    "standards:fix": "npm run quality:fix",
    "standards:enforce": "npm run standards:fix && npm run test",
    "microservices:lint": "find microservices -name '*.js' -o -name '*.ts' | xargs npx eslint",
    "microservices:format": "find microservices -name '*.js' -o -name '*.ts' | xargs npx prettier --write",
    "validate:env": "node -e \"console.log('Environment validation passed')\"",
    "security:audit": "npm audit --audit-level high",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "precommit": "npm run standards:check && npm run security:audit",
    "test:browser": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:browser:chrome": "playwright test --project=chromium",
    "test:browser:firefox": "playwright test --project=firefox", 
    "test:browser:safari": "playwright test --project=webkit",
    "test:browser:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:browser:visual": "playwright test --project=visual-chromium --project=visual-firefox --project=visual-webkit",
    "test:browser:api": "playwright test --project=api-chromium --project=api-firefox --project=api-webkit",
    "test:browser:all": "playwright test",
    "test:browser:headed": "playwright test --headed",
    "test:browser:debug": "playwright test --debug",
    "test:browser:ui": "playwright test --ui",
    "test:browser:report": "playwright show-report",
    "test:browser:update-snapshots": "playwright test --update-snapshots",
    "test:cross-browser": "npm run test:browser:all"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.14.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.56.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "autoprefixer": "^10.4.21",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "critters": "^0.0.23",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.451.0",
    "multer": "^1.4.5-lts.1",
    "next": "15.4.7",
    "nodemailer": "^6.9.16",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.53.2",
    "sharp": "^0.33.5",
    "stripe": "^17.3.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@faker-js/faker": "^8.0.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.19",
    "@types/pg": "^8.15.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "bcrypt": "^6.0.0",
    "eslint": "^8",
    "eslint-config-next": "15.4.7",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^8.0.3",
    "inquirer": "^12.9.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.0",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "pixelmatch": "^7.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "prisma": "^6.14.0",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "uuid": "^11.1.0",
    "zustand": "^5.0.8"
  }
}
